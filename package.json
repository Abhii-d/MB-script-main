{
  "name": "mb-script-main",
  "version": "1.0.0",
  "description": "TypeScript Node.js project for monitoring Muscle Blaze whey protein prices with Telegram alerts",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "start": "node dist/index.js",
    "dev": "npm run build && npm run start",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "vercel:build": "npm run build",
    "test:api": "curl http://localhost:3000/health"
  },
  "keywords": [
    "typescript",
    "nodejs",
    "price-monitoring",
    "telegram-bot",
    "web-scraping"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.10.2",
    "@types/node-telegram-bot-api": "^0.64.9",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "config": {
    "development": {
      "guidelines": "./DEVELOPMENT_GUIDELINES.md",
      "cursorrules": "./.cursorrules",
      "copilotInstructions": "./.github/copilot-instructions.md"
    }
  },
  "dependencies": {
    "axios": "^1.10.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "node-telegram-bot-api": "^0.66.0",
    "winston": "^3.17.0"
  }
}
